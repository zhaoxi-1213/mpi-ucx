PROTOTYPE VOID comm_flush_buffer(COMM comm)
{
    int ierr_c;
    MPI_Comm c_comm;

    c_comm = PMPI_Comm_f2c(*comm);

    ierr_c = @INNER_CALL@(c_comm);
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(ierr_c);
}

